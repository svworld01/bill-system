<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@include file="header.html" %>
<div class="container">
    <h1>Change Your Mobile Number</h1>
    <p>change your mobile number...</p>
    <p></p>
    <div class="row">
        <div class="col-lg-2">
        </div>
        <div class="col-lg-8">
            <form action="" method="post">
                <table class="table table-hover" style="width: 100%;">
                    <tr>
                        <td>Current Mob. No.</td>
                        <td><input type="password" name="opass" class="form-control" placeholder="current mobile number..."></td>
                    </tr>
                    <tr>
                        <td>New Mob. No.</td>
                        <td><input type="password" name="npass" class="form-control" placeholder="new new mobile number..."></td>
                    </tr>
                    <tr>
                        <td>Enter Password</td>
                        <td><input type="password" name="cpass" class="form-control" placeholder="enter your password please.."></td>
                    </tr>
                    <tr>
                        
                        <td colspan="2"><input type="submit" class="form-control btn btn-primary" value="Change Password"></td>
                    </tr>
                </table>
            </form>
        </div>
        <div class="col-lg-2">
            
        </div>
    </div>
</div>
